{"ast":null,"code":"export default {\n  name: \"ProductForm\",\n  data() {\n    return {\n      product: []\n    };\n  },\n  // created là hàm chạy đầu tiên của component này\n  created() {\n    this.getAll();\n  },\n  methods: {\n    getAll() {\n      this.$request.get('http://localhost:3000/product').then(res => {\n        this.product = res.data;\n      });\n    },\n    onDelete(productId) {\n      this.$swal.fire({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\"\n      }).then(result => {\n        if (result.isConfirmed) {\n          this.$request.delete(`http://localhost:3000/product/${productId}`).then(res => {\n            if (res.data) {\n              this.$swal.fire({\n                title: \"Deleted!\",\n                text: \"Your file has been deleted.\",\n                icon: \"success\"\n              });\n              this.getAll();\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","product","created","getAll","methods","$request","get","then","res","onDelete","productId","$swal","fire","title","text","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","result","isConfirmed","delete"],"sources":["D:\\Học kỳ 2 Năm 2\\ungdungwed\\do_an_vue\\src\\views\\product\\ProductList.vue"],"sourcesContent":["<template>\r\n    <div class=\"product_list\">\r\n        <div class=\"pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center\">\r\n            <h3 class=\"display-4\">Product Management</h3>\r\n            <p>\r\n                <router-link to=\"/Product/create\">Add new</router-link>\r\n            </p>\r\n        </div>\r\n\r\n        <div class=\"container\">\r\n            <div class=\"card-deck mb-3\">\r\n                <table class=\"table table-bordered\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th scope=\"col\">#</th>\r\n                            <th scope=\"col\">Product name</th>\r\n                            <th scope=\"col\">Price</th>\r\n                            <th scope=\"col\">Handle</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr :key=\"index\" v-for=\"(item, index) in product\">\r\n                            <th scope=\"row\">{{ item.id }}</th>\r\n                            <td>{{ item.name }}</td>\r\n                            <td>{{ item.price }}</td>\r\n                            <!-- <td>{{ item.description }}</td> -->\r\n                            <td>\r\n                                <!-- <router-link :to=\"{ name: 'product.edit', params: { id: item.id } }\"></router-link> -->\r\n                                <button class=\"btn btn-primary\">Edit</button>\r\n                                <button class=\"btn btn-danger ml-2\" @click=\"onDelete(item.id)\">Delete</button>\r\n\r\n                            </td>\r\n                        </tr>\r\n\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"ProductForm\",\r\n    data() {\r\n        return {\r\n            product: []\r\n        }\r\n    },\r\n    // created là hàm chạy đầu tiên của component này\r\n    created() {\r\n        this.getAll();\r\n    },\r\n    methods: {\r\n        getAll() {\r\n            this.$request.get('http://localhost:3000/product').then(res => {\r\n                this.product = res.data\r\n            })\r\n        },\r\n        onDelete(productId) {\r\n            this.$swal.fire({\r\n                title: \"Are you sure?\",\r\n                text: \"You won't be able to revert this!\",\r\n                icon: \"warning\",\r\n                showCancelButton: true,\r\n                confirmButtonColor: \"#3085d6\",\r\n                cancelButtonColor: \"#d33\",\r\n                confirmButtonText: \"Yes, delete it!\"\r\n            }).then((result) => {\r\n                if (result.isConfirmed) {\r\n                    this.$request.delete(`http://localhost:3000/product/${productId}`).then(res => {\r\n                        if (res.data) {\r\n                            this.$swal.fire({\r\n                                title: \"Deleted!\",\r\n                                text: \"Your file has been deleted.\",\r\n                                icon: \"success\",\r\n                            });\r\n                            this.getAll();\r\n                        }\r\n                    })\r\n                }\r\n            });\r\n        }\r\n    }\r\n\r\n}\r\n</script>"],"mappings":"AA0CA,eAAe;EACXA,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE;IACb;EACJ,CAAC;EACD;EACAC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,MAAM,CAAC,CAAC;EACjB,CAAC;EACDC,OAAO,EAAE;IACLD,MAAMA,CAAA,EAAG;MACL,IAAI,CAACE,QAAQ,CAACC,GAAG,CAAC,+BAA+B,CAAC,CAACC,IAAI,CAACC,GAAE,IAAK;QAC3D,IAAI,CAACP,OAAM,GAAIO,GAAG,CAACR,IAAG;MAC1B,CAAC;IACL,CAAC;IACDS,QAAQA,CAACC,SAAS,EAAE;MAChB,IAAI,CAACC,KAAK,CAACC,IAAI,CAAC;QACZC,KAAK,EAAE,eAAe;QACtBC,IAAI,EAAE,mCAAmC;QACzCC,IAAI,EAAE,SAAS;QACfC,gBAAgB,EAAE,IAAI;QACtBC,kBAAkB,EAAE,SAAS;QAC7BC,iBAAiB,EAAE,MAAM;QACzBC,iBAAiB,EAAE;MACvB,CAAC,CAAC,CAACZ,IAAI,CAAEa,MAAM,IAAK;QAChB,IAAIA,MAAM,CAACC,WAAW,EAAE;UACpB,IAAI,CAAChB,QAAQ,CAACiB,MAAM,CAAE,iCAAgCZ,SAAU,EAAC,CAAC,CAACH,IAAI,CAACC,GAAE,IAAK;YAC3E,IAAIA,GAAG,CAACR,IAAI,EAAE;cACV,IAAI,CAACW,KAAK,CAACC,IAAI,CAAC;gBACZC,KAAK,EAAE,UAAU;gBACjBC,IAAI,EAAE,6BAA6B;gBACnCC,IAAI,EAAE;cACV,CAAC,CAAC;cACF,IAAI,CAACZ,MAAM,CAAC,CAAC;YACjB;UACJ,CAAC;QACL;MACJ,CAAC,CAAC;IACN;EACJ;AAEJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}